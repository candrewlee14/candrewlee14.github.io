		"autoprefixer": "^10.4.2",
		"dayjs": "^1.10.7",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"mdsvex": "^0.10.5",
